<?php

$isHhvm = defined('HHVM_VERSION');
$isPhp7 = version_compare(PHP_VERSION, '7.0') >= 0;

if ($isHhvm && $isPhp7 && !interface_exists('\Throwable', false)) {
    /**
     * @internal
     */
    interface Throwable
    {
    }

    require_once __DIR__ . DIRECTORY_SEPARATOR . 'StackTraceException.php.polyfill';
}
